
config basic
	option program_dir '/usr/bin'
	option library_dir '/usr/lib'
	option enable '1'
	option profile_dir '/mnt/mmcblk0p1'

config Preferences
	option port '8080'
	option download_dir '/mnt/mmcblk0p1/all/Downloads'

config qbittorrent 'main'
	option user 'root'
	option BTProtocol 'Both'
	option DisableAutoTMMByDefault 'true'
	option CategoryChanged 'false'
	option DefaultSavePathChanged 'true'
	option CategorySavePathChanged 'true'
	option Encryption '0'
	option MaxRatioAction '0'
	option SeedChokingAlgorithm 'FastestUpload'
	option SavePath '/mnt/mmcblk0p1/all/Downloads'
	option profile '/mnt/mmcblk0p1'
	option Locale 'zh'
	option UseUPnP 'true'
	option Username 'admin'
	option Password 'qwerqwe123'
	option AnnounceToAllTrackers 'true'

